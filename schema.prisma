datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

generator client {
  provider = "prisma-client-js"
  output = "./node_modules/.prisma/client"
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  username  String
  fullName  String
  email     String   @unique
  password  String
  createdAt DateTime @default(now()) @map("created_at")
  dateOfBirth DateTime @default(now()) @map("date_of_birth")
}


