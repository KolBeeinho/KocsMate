{
  "name": "template-nextjs-tailwind",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "yarn dev",
    "dev": "(cls || clear) && yarn prisma generate && concurrently \"yarn:dev:*\"",
    "dev:next": "next dev",
    "dev:tailwind": "npx tailwindcss -o src/styles/build.css --minify --watch",
    "build": "yarn build:tailwind && yarn build:next",
    "build:next": "next build",
    "build:tailwind": "npx tailwindcss -o src/styles/build.css --minify --watch",
    "update:all": "yarn upgrade-interactive",
    "generate": "yarn prisma generate"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.21.1",
    "@types/maplibre-gl": "^1.14.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "heroicons": "^2.1.5",
    "maplibre-gl": "^4.7.1",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.0",
    "next": "^15.0.2",
    "next-auth": "^4.24.10",
    "prisma": "^5.21.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-map-gl": "^7.1.7",
    "sass": "^1.80.5",
    "tailwindcss": "^3.4.4",
    "template-nextjs-tailwind": "file:"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@types/bcryptjs": "^2",
    "@types/express": "^4",
    "@types/node": "18.19.39",
    "@types/react": "18.3.3",
    "concurrently": "^8.2.2",
    "fluid-tailwind": "^1.0.3",
    "postcss": "8.4.39",
    "supervisor": "^0.12.0",
    "typescript": "5.5.3"
  },
  "packageManager": "yarn@4.0.1",
  "engines": {
    "node": ">=18.17.0"
  }
}
