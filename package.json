{
  "name": "kocsmate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn dev",
    "dev": "(cls || clear) && yarn prisma generate && concurrently \"yarn:dev:*\"",
    "dev:next": "next dev",
    "dev:tailwind": "npx tailwindcss -o src/styles/build.css --minify --watch",
    "build": "(cls || clear) && npx cap sync && yarn build:next && npx cap sync",
    "build:next": "next build",
    "build:tailwind": "npx tailwindcss -o src/styles/build.css --minify --watch",
    "update:all": "yarn upgrade-interactive",
    "generate": "yarn prisma generate"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.1",
    "@capacitor/cli": "^7.0.1",
    "@capacitor/core": "^7.0.1",
    "@capacitor/device": "^7.0.0",
    "@capacitor/geolocation": "^7.0.0",
    "@capacitor/ios": "^7.0.1",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.3.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/maplibre-gl": "^1.14.0",
    "argon2": "^0.41.1",
    "caniuse-lite": "^1.0.30001695",
    "dotenv": "^16.4.7",
    "fs": "^0.0.1-security",
    "heroicons": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "kocsmate": "file:",
    "mapbox-gl": "^3.9.3",
    "maplibre-gl": "4.7.1",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.5",
    "multer": "^1.4.5-lts.1",
    "next": "^15.1.6",
    "next-auth": "^4.24.11",
    "next-connect": "^1.0.0",
    "nodemailer": "^6.10.0",
    "prisma": "^6.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-map-gl": "^7.1.8",
    "tailwindcss": "^4.0.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/multer": "^1",
    "@types/node": "^22.10.10",
    "@types/nodemailer": "^6",
    "@types/react": "^19.0.8",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.1",
    "supervisor": "^0.12.0",
    "typescript": "^5.7.3"
  },
  "packageManager": "yarn@4.6.0"
}
